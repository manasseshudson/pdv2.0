	<script>
		const campo = document.getElementById('valor_abertura_caixa');
		const campoFechamento = document.getElementById('valorfechamentoCaixa');
		const campoSaidas = document.getElementById('valor_saida');
		

		campo.addEventListener('input', () => {
		  let valor = campo.value.replace(/\D/g, ''); // remove tudo que não é número
		  valor = (valor / 100).toFixed(2) + ''; // divide por 100 e fixa 2 casas
		  valor = valor.replace('.', ','); // troca ponto por vírgula
		  //valor = 'R$ ' + valor.replace(/\B(?=(\d{3})+(?!\d))/g, '.'); // formata milhar
		  valor = valor.replace(/\B(?=(\d{3})+(?!\d))/g, '.'); // formata milhar
		  valor = valor.replace(',', '.');
		  campo.value = valor;
		});

		campoFechamento.addEventListener('input', () => {
		  let valor = campoFechamento.value.replace(/\D/g, ''); // remove tudo que não é número
		  valor = (valor / 100).toFixed(2) + ''; // divide por 100 e fixa 2 casas
		  valor = valor.replace('.', ','); // troca ponto por vírgula
		  //valor = 'R$ ' + valor.replace(/\B(?=(\d{3})+(?!\d))/g, '.'); // formata milhar
		  valor = valor.replace(/\B(?=(\d{3})+(?!\d))/g, '.'); // formata milhar
		  valor = valor.replace(',', '.');
		  campoFechamento.value = valor;
		});

		campoSaidas.addEventListener('input', () => {
		  let valor = campoSaidas.value.replace(/\D/g, ''); // remove tudo que não é número
		  valor = (valor / 100).toFixed(2) + ''; // divide por 100 e fixa 2 casas
		  valor = valor.replace('.', ','); // troca ponto por vírgula
		  //valor = 'R$ ' + valor.replace(/\B(?=(\d{3})+(?!\d))/g, '.'); // formata milhar
		  valor = valor.replace(/\B(?=(\d{3})+(?!\d))/g, '.'); // formata milhar
		  valor = valor.replace(',', '.');
		  campoSaidas.value = valor;
		});

</script>

	<script type="text/javascript">
		$(document).ready(function() {
		
		

		
		
		let documento_interno_ = "";
		
		$.get("/user/abertura_caixa/"+"<%= uid_usuario %>"+"", function(data, status){
			console.log(data.mensagem)
			
			if(data.mensagem=="nao houve abertura"){
				alert("Atenção fazer abertura de caixa...");
				$('#modalAberturaCaixa').modal({
				  backdrop: 'static', // impede fechar clicando fora
				  keyboard: false     // impede fechar com ESC
				});

				$('#modalAberturaCaixa').modal('show');
				
			}
		});
		
		$('#realizarFechamentoCaixa').click(function(){
			let valor = $("#valorfechamentoCaixa").val();	
			let uid = $("#uid_usuario").val();			
			let id_empresa = $("#id_empresa").val();
			$.post('/user/fechamentoCaixa', {
				uid_usuario: uid,
				id_empresa: id_empresa,
				valor: valor
			}, function(resposta) {
				alert(resposta.mensagem)
				window.location.href = "/";
				
			});
			
		})
		
		$("#realizarAberturaCaixa").click(function(){
			let valor = $("#valor_abertura_caixa").val();	
			let uid = $("#uid_usuario").val();			
			let id_empresa = $("#id_empresa").val();			
			
			if(valor==""){
				swal("Informe um Valor");
