<div class="modal" tabindex="-1" id="modalCadastrarProdutos">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title">Cadastro de Usuários</h5>
			</div>
			<div class="modal-body">
			
				<form id="formulario" action="javascript:func()" method="post" class="form-horizontal">
					<div class="col-md-12">
						<div class="form-group">
							  <label for="inputEmail3" class="col-sm-2 control-label">Nome</label>
							  <div class="col-sm-10">
									<input type="text" class="form-control" placeholder="" id="nome" required />
							  </div>
						</div>
					</div>
					<div class="col-md-12">
						<div class="form-group">
							  <label for="inputEmail3" class="col-sm-2 control-label">Email</label>
							  <div class="col-sm-10">
									<input type="text" class="form-control" placeholder="" id="email" required />
							  </div>
						</div>
					</div>
					<div class="col-md-12">
						<div class="form-group">
							<label for="inputEmail3" class="col-sm-2 control-label">Senha</label>
							<div class="col-sm-3">
									<input type="password" class="form-control" placeholder="" id="senha" required />
							</div>
							<div class="col-sm-3" style="margin-top:7px;">
								<div class="form-check">
									<input class="form-check-input" type="checkbox" value="" id="funcionario" checked>
									<label class="form-check-label" for="funcionario">Funcionário</label>
								</div>
							</div>
							  
						</div>
					</div>
					
				</form>
			
			
			</div>
			<div class="modal-footer">
				<button type="button" id="fecharModal" class="btn btn-secondary" data-bs-dismiss="modal">Fechar</button>
				<button type="submit" class="btn btn-primary" id="cadastrarUsuarios">Cadastrar</button>
			</div>
    </div>
  </div>
</div>
